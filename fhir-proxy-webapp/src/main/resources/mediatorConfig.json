{
  "urn": "urn:mediator:tutorial_cs",
  "version": "1.0.0",
  "name": "Consolidated Server Mediator",
  "description": "Mediator for processing and logging check ins to the Consolidated Server",
  "defaultChannelConfig": [
    {
      "name": "Consolidated Server Mediator",
      "urlPattern": "^/cs.*$",
      "routes": [
        {
          "name": "Consolidated Server Mediator Route",
          "host": "host.openelis.org",
          "port": "9080",
          "primary": true,
          "type": "http"
        }
      ],
      "allow": ["admin"],
      "methods": ["GET", "POST", "PUT"],
      "type": "http"
    }
  ],
  "endpoints": [
    {
      "name": "Consolidated Server Mediator Endpoint",
      "host": "host.openelis.org",
      "port": "9080",
      "primary": true,
      "type": "http"
    }
  ]
}
